{
  "name": "@manahippo/move-to-ts",
  "version": "0.2.29",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "build-rollup": "rm -rf dist && rollup -c",
    "build-test": "rm -rf tests && rollup -c rollup.config.test.js",
    "test-cjs": "node tests/test.js test",
    "test-mjs": "node tests/test.mjs test",
    "test": "yarn build-test && yarn test-cjs && yarn test-mjs"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "rollup": "^2.79.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/elliptic": "^6.4.14",
    "@types/json-stable-stringify": "^1.0.34",
    "@types/node": "^17.0.31",
    "@types/sha.js": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "aptos": "1.3.12",
    "big-integer": "^1.6.51",
    "elliptic": "^6.5.4",
    "json-stable-stringify": "^1.0.1",
    "path": "^0.12.7",
    "sha.js": "^2.4.11",
    "sha3": "^2.1.4",
    "tslib": "^2.4.0"
  }
}
